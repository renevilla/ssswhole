/* Accordion */


.accordion {
  float: left;
  display: block;
  width: 1000px;
  margin-top: 35px;
  margin-left: 40px;
}

.accordion h1 {
  margin-bottom: 20px;
}

.accordion h3 {
  font-family: 'proxima-regular', helvetica;
  font-size: 22px;
  font-weight: 100;
  padding: 20px 0;
  cursor: pointer;
  letter-spacing: 1px;
  outline: none;
}

.accordion h3:hover {
  color: #ccc;
}

.accordion div p {
  margin: 0 35px 20px 35px;
  letter-spacing: .11em;

}

h3.accordion-title {
  text-transform: uppercase;
  font-family: 'proxima-semibold', helvetica;
  font-weight: bold;
  margin-top: 28px;
  margin-bottom: 20px;
  float: left;
  display: block;
  clear: both;
  width: 1000px;
}

h3.accordion-title:hover {
  color: #000;
}

